[sub_resource type="GDScript" id="GDScript_4bxh6"]
script/source = "extends TileMapLayer

@onready var overlay: TileMapLayer = $\"../overlay\"
@onready var animation_timer: Timer = $\"../../AnimationTimer\"
@onready var creatures_map: TileMapLayer = $\".\"

const ANIMAL_SOURCE_ID = 4
const OVERLAY_SOURCE_ID = 0

const MAX_ANIMALS_PER_GAME = 9
const GAME_BOARD_SIZE = 9

var animal_map = Dictionary()

var total_rounds = 0
var time_elapsed = 0
var animation_ticks = 0
var grid = []
var animals = []
var lamps = []

var last_animal_selected: Animal = null

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	var random = RandomNumberGenerator.new()
	random.randomize()
	
	create_grid_map()
	create_lamps()
	
	#Init 2d Array
	for i in range(GAME_BOARD_SIZE):
		animals.append([])
		for j in range(GAME_BOARD_SIZE):
			animals[i].append(0)
		
	for i in range(GAME_BOARD_SIZE):
		for j in range(GAME_BOARD_SIZE):
			place_animal(Vector2i(i, j))


func create_grid_map():	
	for i in range(0, GAME_BOARD_SIZE):
		var g: Grid = Grid.new(i)
		lamps.append(g.new_lamp())
		grid.append(g)
func create_lamps():
	for lamp: Lamp in lamps:
		lamp.render_lamp(self)
	
func place_animal(location: Vector2i) -> void:
	var random_animal_id = Vector2i((randi() % MAX_ANIMALS_PER_GAME), 0)
	
	
	if animal_map.has(random_animal_id):
		if animal_map.get(random_animal_id) < MAX_ANIMALS_PER_GAME:
			animal_map[random_animal_id] = animal_map[random_animal_id]+1
			var animal = Animal.new(self, random_animal_id.x, location)
			animals[location.x][location.y] = (animal)
		else:
			place_animal(location)
	else:
		animal_map[random_animal_id] = 1
		var animal = Animal.new(self, random_animal_id.x, location)
		animals[location.x][location.y] = (animal)
	
	

func _input(event: InputEvent) -> void:
	if event is InputEventMouseButton and event.is_pressed() and event.button_index == MOUSE_BUTTON_LEFT:
		var clicked_position = event.position
		var grid_position = local_to_map(clicked_position)
		
		if(grid_position.y > GAME_BOARD_SIZE-1 or grid_position.x > GAME_BOARD_SIZE-1):
			return
		if(grid_position.y < 0 or grid_position.x < 0):
			return
			
		var clicked_animal_new: Animal = animals[grid_position.x][grid_position.y]
		
		if last_animal_selected != null:
			if last_animal_selected.grid_position.x == grid_position.x and last_animal_selected.grid_position.y == grid_position.y:
				reset_overlay()
				unselect_animal(last_animal_selected)
				last_animal_selected = null
				return
			if overlay.get_cell_atlas_coords(grid_position).x == 1: ## jedna je modry overlay
				swap_animals(last_animal_selected, clicked_animal_new)
			reset_overlay()
			unselect_animal(last_animal_selected)
			last_animal_selected = null
			
		else:
			last_animal_selected = clicked_animal_new
			select_animal(last_animal_selected)
			overlay.set_cell(grid_position, OVERLAY_SOURCE_ID, Vector2i(0, 0))
			create_overlay_shadow(grid_position)
			print(\"Animal selected: \", get_animal_from_id(clicked_animal_new.id))

func select_animal(animal: Animal):
	#animal.rotation = 180
	animation_timer.start()
	pass
func unselect_animal(animal: Animal):
	check_win()
	#animal.rotation = 0
	animation_timer.stop()
	animal.rotation = 0
	animation_ticks = 0
	pass

func swap_animals(last: Animal, new: Animal):
	var last_pos: Vector2i = last.position
	var new_pos: Vector2i = new.position
	
	var last_grid_pos = last.grid_position
	var new_grid_pos = new.grid_position
	
	last.position = new_pos
	new.position = last_pos
	
	animals[last_grid_pos.x][last_grid_pos.y] = new
	animals[new_grid_pos.x][new_grid_pos.y]= last
	
	last.grid_position = new_grid_pos
	new.grid_position = last_grid_pos
func create_overlay_shadow(current: Vector2i):
	for i in range(GAME_BOARD_SIZE):
		for j in range(GAME_BOARD_SIZE):
			if(current.x != i or current.y != j):
				overlay.set_cell(Vector2i(i, j), OVERLAY_SOURCE_ID, Vector2i(2, 0))
	for i in range(-1,2):
		for j in range(-1, 2):
			var new_x = current.x + i
			var new_y = current.y + j
			if new_x > -1 && new_x < 9 && new_y > -1 && new_y < 9:
				if(current.x != new_x or current.y != new_y):
					overlay.set_cell(Vector2i(new_x, new_y), OVERLAY_SOURCE_ID, Vector2i(1,0))
func reset_overlay():
	for i in range(GAME_BOARD_SIZE):
		for j in range(GAME_BOARD_SIZE):
			overlay.set_cell(Vector2i(i, j), OVERLAY_SOURCE_ID, Vector2i(-1, -1))

func check_win():
	var all_complete: bool = check_all_complete()
	
	var moth_lamp_correct: bool = check_moth_lamps()
	
	if not all_complete:
		return
	if not moth_lamp_correct:
		return
	var spider_flys_correct: bool = check_spider_flys()

	if not spider_flys_correct:
		return
		
	on_win()
			

func on_win():
	print(\"You have won kekw\")	
func get_animal_from_id(animal: int):
	match animal:
		0:
			return \"Mole\" #\"Snail\"
		1:
			return \"Bat\"
		2:
			return \"Hamster\" #\"Fly\"
		3:
			return \"Moth\" #\"Bee\"
		4:
			return \"Snail\" #\"Mole\"
		5:
			return \"Spider\" #\"Hamster\"
		6:
			return \"GrassHopper\"
		7:
			return \"Fly\" #\"Spider\"
		8:
			return \"Bee\"
		_:
			return \"KEY_UNKNOWN\"
func check_all_complete():
	var completed = 0
	for g: Grid in grid:
		var has_all: bool = check_grid_has_all(g.grid_pos.x, g.grid_pos.y)
		if has_all:
			g.has_all = true
			color_grid(g.grid_id, 3)
			completed = completed+1
		else:
			g.has_all = false
	if completed == 9:
		return true
	return false
func color_grid(grid_number: int, grid_color: int):
	for i in range(3):
		for j in range(3):
			var grid_row: int = int( grid_number / 3)
			var grid_col: int = int( grid_number % 3)
			
			var grid_offset_x: int = int( grid_row * 3 )
			var grid_offset_y: int = int( grid_col * 3 )
			overlay.set_cell(Vector2i(j+grid_offset_x, i+grid_offset_y), OVERLAY_SOURCE_ID, Vector2i(grid_color, 0))
func check_grid_has_all(grid_row, grid_col) -> bool:
	var animal_map = Dictionary()
	for i in range(3):
		for j in range(3):
			
			var grid_offset_x: int = int( grid_row * 3 )
			var grid_offset_y: int = int( grid_col * 3 )
			var animal: Animal = animals[j+grid_offset_x][i+grid_offset_y]
			if(animal_map.has(animal.id)):
				continue
			else:
				animal_map[animal.id] = 1
	if(animal_map.size() == 9):
		return true
	return false

func check_moth_lamps() -> bool:
	var grids_ok = 0
	for g: Grid in grid:
		var light_pos = g.light_pos
		var grid_number = g.grid_id
		
		var valid_space = []
		valid_space.append(light_pos - Vector2i(0, 0))
		valid_space.append(light_pos - Vector2i(0, 1))
		valid_space.append(light_pos - Vector2i(1, 0))
		valid_space.append(light_pos - Vector2i(1, 1))
		

		var moth_checked: bool = check_grid_moth(grid_number, valid_space, g.grid_pos.x, g.grid_pos.y)
		if(moth_checked):
			print(\"Moth correct... \" + str(g.grid_id))
			if(g.has_all):
				print(g.light_pos)
				color_grid(grid_number, 4)
			else:
				print(\"does not have all!!!\")
			grids_ok = grids_ok + 1
	if grids_ok == 9:
		return true
		
	return false
		
func check_grid_moth(grid_number: int, valid_space: Array, grid_row, grid_col) -> bool:
	var to_return = false
	for i in range(3):
		for j in range(3):
			
			var grid_offset_x: int = int( grid_row * 3 )
			var grid_offset_y: int = int( grid_col * 3 )
			for valid in valid_space:
				if valid.x == j and valid.y == i:
					var animal_at: Animal = animals[j+grid_offset_x][i+grid_offset_y]
					if(animal_at.id == AnimalType.AnimalType.BAT):
						return false
					if(animal_at.id == AnimalType.AnimalType.MOTH):
						to_return = true
	return to_return
func check_spider_flys() -> bool:
	return true


func _on_animation_timer_timeout() -> void:
	if animation_ticks < 360:
		animation_ticks += 1
	else:
		animation_ticks = -360
	last_animal_selected.rotation_degrees = sin(animation_ticks) * 12
	#pass # Replace with function body.


func _on_game_time_timeout() -> void:
	time_elapsed += 1
"

